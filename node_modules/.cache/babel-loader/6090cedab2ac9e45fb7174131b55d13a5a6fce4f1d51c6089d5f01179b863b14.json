ft__question:nth-child(2) {\n          margin: 0 0 24px 56px;\n        }\n      }\n    }\n  }\n  .right {\n    .right__title {\n      font-weight: 700;\n      font-size: 24px;\n      color: #004ca9;\n      margin-top: 229px;\n      margin-bottom: 19px;\n    }\n    .right-input {\n      width: 650px;\n      height: 47px;\n      background-color: #ffffff;\n      border-radius: 4px;\n      position: relative;\n    }\n    .right__checking {\n      position: absolute;\n      right: 20px;\n      top: 17px;\n      div {\n        width: 20px;\n        height: 20px;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n      }\n    }\n    .right__name,\n    .right__error {\n      margin-bottom: 9px;\n    }\n    .right__mail {\n      margin-bottom: 1px;\n    }\n    .right__message {\n      background-color: #ffffff;\n      border-radius: 4px;\n      width: 100%;\n      padding: 12px 18px;\n      font-size: 16px;\n      line-height: 24px;\n      margin-bottom: 29px;\n      resize: none;\n    }\n    .right__message::placeholder {\n      font-size: 16px;\n      line-height: 150%;\n      color: #849dba;\n    }\n    .right__error {\n      font-family: \"Source Sans Pro\";\n      font-weight: 400;\n      font-size: 14px;\n      line-height: 150%;\n      color: #dd7070;\n    }\n    .right__buttons {\n      display: flex;\n      justify-content: space-between;\n      margin-bottom: 27px;\n      div {\n        border-radius: 50px;\n        text-transform: uppercase;\n        padding: 20px 50px;\n        font-weight: 600;\n        font-size: 16px;\n        line-height: 19px;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        cursor: pointer;\n      }\n      .right__add {\n        color: #3777f3;\n        background-color: #ffffff;\n        border: 3px solid #3777f3;\n      }\n\n      .right__submit {\n        color: #ffffff;\n        background-color: #3777f3;\n        &:hover {\n          transition: 0.5s